{
  "name": "@students-dev/audify-js",
  "version": "1.0.0",
  "description": "Modern modular audio engine with queue, filters, plugin API, and event-driven design.",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "audio",
    "engine",
    "playback",
    "queue",
    "filters",
    "plugins",
    "javascript",
    "nodejs",
    "browser"
  ],
  "author": "Students Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/students-dev/audify-js.git"
  },
  "bugs": {
    "url": "https://github.com/students-dev/audify-js/issues"
  },
  "homepage": "https://github.com/students-dev/audify-js#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}