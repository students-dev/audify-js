{
  "name": "@students-dev/audify-js",
  "version": "1.0.2",
  "description": "Modern modular audio engine with queue, filters, plugin API, and event-driven design.",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "audio",
    "engine",
    "playback",
    "queue",
    "filters",
    "plugins",
    "javascript",
    "nodejs",
    "browser"
  ],
  "author": "Students Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/students-dev/audify-js.git"
  },
  "bugs": {
    "url": "https://github.com/students-dev/audify-js/issues"
  },
  "homepage": "https://github.com/students-dev/audify-js#readme",
  "dependencies": {
    "lavalink-client": "^2.5.1",
    "spotify-web-api-node": "^5.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/spotify-web-api-node": "^5.0.11",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
